
pipeline {
  agent any

  tools {nodejs "NODEJS"}
    
 
  stages {

    stage('Pre-cleanup') {
      steps {
        sh 'rm -rf ./node_modules'
      }
    }

    stage('install node') {
        steps {
        
        sh '''
          npm install
        '''
      }
    }
    stage('help') {
      steps {
        sh 'npx playwright test --help'
      }
    }
    stage('Run tests'){
      steps {
          sh 'npm run testcase'
      }
    }
  }
}

