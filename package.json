{
  
  "name": "playwrightgherkin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "type": "module",
    "pretest": "npx ts-node src/helper/report/init.ts",
    "testJenkins": "cucumber-js --require cucumberJenkins.ts --require src/features/**/*.feature --require src/test/steps/**/*.ts",
    "jenkinsTag": "npm run testJenkins",
    "test": "cucumber-js test",
    "posttest": "npx ts-node src/helper/report/report.ts",
    "test:failed": "cucumber-js -p rerun @rerun.txt",
    "tag": "npm test -- -t"
  },
  "keywords": [],
