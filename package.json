    "@cucumber/cucumber": "^9.1.2",
    "@playwright/test": "^1.35.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.0",
    "fs-extra": "^11.1.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "multiple-cucumber-html-reporter": "^3.4.0",
    "npm": "^9.7.2",
    "winston": "^3.9.0"
  }
}
